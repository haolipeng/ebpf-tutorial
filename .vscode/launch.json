{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug helloworld (GDB)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/src/helloworld",
            "args": [],
            "cwd": "${workspaceFolder}/src",
            "stopAtEntry": false,
            "externalConsole": false,
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Debug kprobe (GDB)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/src/kprobe",
            "args": [],
            "cwd": "${workspaceFolder}/src",
            "stopAtEntry": false,
            "externalConsole": false,
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Debug uprobe (GDB)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/src/uprobe",
            "args": ["./target"],
            "cwd": "${workspaceFolder}/src",
            "stopAtEntry": false,
            "externalConsole": false,
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
